<p id="notice"><%= notice %></p>
<div class="header">
  <div class="container">
    <img src="https://www.freelogoservices.com/api/main/images/1j+ojl1FOMkX9WypfBe43D6kjPCCqhZKnx...JwXs1M3EMoAJtlCkuh...Rs...f46">
    <h1>Messages</h1>
  </div>
</div>

<div class="newmessage">
  <%= link_to 'New Message', new_message_path %>
</div>

  <div class="messages">
    <div class="messcontent">
    <% @messages.each do |message| %>
        <div class="message">
          <p class="content">
            <%= message.content %></p>  
          <p class="time"><%= message.created_at %></p>
          <%= link_to 'Show', message %>
          <%= link_to 'Edit', edit_message_path(message) %>
          <%= link_to 'Destroy', message, method: :delete, data: { confirm: 'Are you sure?' } %>
        </div>
    <% end %>
  </div>
</div>


